{
  "name": "bytic/cache",
  "type": "library",
  "description": "Cache module for bytic framework",
  "homepage": "https://github.com/bytic/cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "provide": {
    "psr/cache-implementation": "1.0",
    "psr/simple-cache-implementation": "1.0",
    "symfony/cache-implementation": "1.0"
  },
  "require": {
    "bytic/package-base": "^1.0",
    "bytic/utility": "^1.0",
    "psr/cache": "^1.0",
    "psr/log": "^1.0",
    "psr/simple-cache": "^1.0",
    "symfony/cache": "^4.3|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "mockery/mockery": "^1.0",
    "bytic/container": "^0.9|^1.0|^2.0",
    "bytic/config": "^0.9|^1.0"
  },
  "autoload": {
    "psr-4": {
      "Nip\\Cache\\": "src"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/application.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Cache\\Tests\\": "tests/src",
      "Nip\\Cache\\Tests\\Fixtures\\": "tests/fixtures"
    }
  }
}
