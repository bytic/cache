{
  "name": "bytic/cache",
  "type": "library",
  "description": "Cache module for bytic framework",
  "homepage": "https://github.com/bytic/cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Cache\\": "src"
    },
    "classmap": [
      "src"
    ],
    "files": [
      "src/functions/application.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Cache\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/filesystem": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9"
  }
}
